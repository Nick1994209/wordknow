http {
    gzip              on;
    gzip_http_version 1.0;
    gzip_proxied      any;

    server {
        listen 80;

        location /static/ {
            root /app_static_files;
        }

        location / {
            include proxy_params;
            proxy_pass unix:/code/wordknow/application/app.sock;
        }
    }

}
